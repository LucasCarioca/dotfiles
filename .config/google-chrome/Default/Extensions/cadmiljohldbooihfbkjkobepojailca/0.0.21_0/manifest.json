{
   "author": "Sriram Thiagarajan",
   "background": {
      "scripts": [ "js/twitter-night-mode-bg.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/twitter-night-mode.js" ],
      "matches": [ "https://twitter.com/*", "https://twitter.com/", "https://twitter.com/i/cards/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": false,
      "js": [ "js/notification.js" ],
      "matches": [ "https://twitter.com/" ],
      "run_at": "document_end"
   } ],
   "description": "Android/iOS like night mode for twitter web app",
   "homepage_url": "https://github.com/tsriram/twitter-web-night-mode",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "32": "images/icon32.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Twitter Web - Night Mode",
   "page_action": {
      "default_icon": "images/icon32.png",
      "default_title": "Extension will be enabled when you open twitter.com"
   },
   "permissions": [ "https://twitter.com/", "tabs", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.21",
   "web_accessible_resources": [ "css/main.css" ]
}
